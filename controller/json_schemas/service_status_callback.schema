{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "service_name": {
      "type": "string"
    },
    "key": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "READY",
        "FAIL",
        "TIMEOUT",
        "IN_PROCESS"
      ]
    },
    "response": {
      "type": "object"
    }
  },
  "required": [
    "service_name",
    "key",
    "status"
  ]
}
